 </nav>
 </header>
 <section class="advertise">

 </section>
 <h2>List of Cars</h2>
 <div class = "container">

    <% cars.forEach(car => { %>
    <div class="car">
      <img src="<%=car.img[0]%>"  height="150px" width="150px" alt="" srcset="">
      <%= car.secondid.lenght %>
      <div class="name">
      <a href="/cars/<%= car._id %>"><%= car.name %> </a>
      </div>  
      <div class='text'>
      Price: <%= car.price %>
      </div>  
    </div>
      <% }) %>

</div>
<footer class = "footer">

</footer>
</body>
</html>